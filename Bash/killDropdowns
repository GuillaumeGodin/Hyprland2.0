# #!/bin/bash

hyprctl keyword unbind , mouse:273
hyprctl keyword unbind , escape

hyprctl keyword unbind CONTROL, CONTROL_L
hyprctl keyword bind CONTROL, CONTROL_L, exec, bash Hyprland2.0/HyprV/waybar/scripts/buttons Scripts

hyprctl keyword unbind SUPER, SUPER_L
hyprctl keyword bind SUPER, SUPER_L, exec, bash Hyprland2.0/HyprV/waybar/scripts/buttons Apps

hyprctl keyword unbind ALT, ALT_L
hyprctl keyword bind ALT, ALT_L, exec, bash Hyprland2.0/HyprV/waybar/scripts/buttons Files

hyprctl dispatch killwindow class:Tk